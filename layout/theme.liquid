  {% include 'google-fonts' %}

  {% capture chevron_left %}{% include 'svg' with 'chevron_left' %}{% endcapture %}
  {% capture chevron_right %}{% include 'svg' with 'chevron_right' %}{% endcapture %}
  {% capture play_arrow %}{% include 'svg' with 'play-arrow' %}{% endcapture %}

  {% capture money_format %}{% raw %}<span class=money>${{amount_no_decimals}}</span>{% endraw %}{% endcapture %}

  <script>
    var totallee = totallee || {}
    var theme = theme || {}

    totallee.metafieldsById = {}
    totallee.addonsBySize = {}
    totallee.productEntity = {}
    totallee.upsell = {}

    theme.strings = {
      addToCart: {{ 'products.product.add_to_cart' | t | json }},
      soldOut: {{ 'products.product.sold_out' | t | json }},
      unavailable: {{ 'products.product.unavailable' | t | json }},
      showMore: {{ 'general.filters.show_more' | t | json }},
      showLess: {{ 'general.filters.show_less' | t | json }},
      cartEmpty: {{ 'cart.general.empty' | t | json }},
    }
    theme.moneyFormat = {{ money_format | json }}
    theme.assets = {
      chevronLeft: {{ chevron_left | json }},
      chevronRight: {{ chevron_right | json }},
      playArrow: {{ play_arrow | json }},
    }
    theme.upsell = {
      enable: {{ settings.upsell | json }},
      page: {{ settings.upsell_page | json }},
      id: {{ all_products[settings.upsell_product].id | json }},
      variantId: {{ settings.upsell_variant_id | json }},
    }
    theme.cart = {{ cart | json }}

    document.documentElement.className = document.documentElement.className.replace('no-js', 'js');

    {% for i in (1..20) %}
      {% assign key = 'upsell_product_' | append: i | append: '_key' %}
      {% assign a = 'upsell_product_' | append: i | append: '_a' %}
      {% assign b = 'upsell_product_' | append: i | append: '_b' %}

      {% if settings[a] != blank and settings[b] != blank and settings[key] != blank %}
        totallee.upsell[ {{ settings[key] | downcase | json }} ] = [
          {{ settings[a] | downcase | json }},
          {{ settings[b] | downcase | json }}
        ]
      {% endif %}
    {% endfor %}
  </script>



